[user]
    name = Tzadik Vanderhoof
    email = tzadik.vanderhoof@gmail.com
[alias]
    cam = commit -am
    cm = commit -m
    lg = log --graph --oneline --decorate --pretty=format:\"%h%x09%an%x09%ar%x09%s%x09%d\"
    lg1 = log --graph --oneline --decorate --pretty=format:\"%h%x09%an%x09%ar%x09%s%x09%d\" -1
    lgp = log --graph --oneline --decorate --pretty=format:\"%h%x09%an%x09%ar%x09%s%x09%d\" -p
    co = checkout
    cob = checkout -b
    lgg = log --graph --oneline --decorate --all --pretty=format:\"%h%x09%an%x09%ar%x09%s%x09%d\"
    st = status -sb
    df = diff --color
    dfb = diff --color -b
    dfc = diff --color --cached
    dfcb = diff --color --cached -b
    dfn = diff --color --name-status
    lgn = log --oneline --decorate --pretty=format:\"%h%x09%an%x09%ar%x09%s%x09%d\" -p --name-status
    lgn1 = log --oneline --decorate --pretty=format:\"%h%x09%an%x09%ar%x09%s%x09%d\" -p --name-status -1
    cp = cherry-pick
    shn = show --name-only
    fp = fetch --prune
    pushnew = push -u origin HEAD
    pushnewfl = push --force-with-lease -u origin HEAD
    pushnewf = push -f -u origin HEAD
    amend = commit --amend --no-edit --reset-author
    bd = branch -d
    bD = branch -D
    pod = push origin --delete
    b = branch
    pushfl = push --force-with-lease
    ff = merge --ff-only
    det = checkout --detach
[core]
    preloadindex = true
    ignoreStat = false
    safecrlf = false
    autocrlf = false
    pager = tr -d '\\r' | less -REX
    excludesfile = ~/config/dot.git.ignore
[gc]
    auto = 256
[credential]
    helper = wincred
[http]
#   sslVerify = false
[alias]
    p4 = !~/bin/git-p4.py
